<div class="card">
  <div class="card-body">
    <div class="card-title">
      <h5>Response</h5>
    </div>
    <div class="card-text">
      <div id="requestError" class="alert alert-danger" role="alert" style="display: none">
        This is a danger alert—check it out!
      </div>

      <div class="mb-3">
        <label>Code</label>
        <input id="responseCodeInput" type="text" class="form-control" readonly />
      </div>
      <div class="mb-3">
        <label>Message</label>
        <input id="responseMessageInput" type="text" class="form-control" readonly />
      </div>
      <div class="mb-3">
        <label>Vodafone Cost</label>
        <input id="responseVodafoneCostInput" type="text" class="form-control" readonly />
      </div>
      <div class="mb-3">
        <label>Orange Cost</label>
        <input id="responseOrangeCostInput" type="text" class="form-control" readonly />
      </div>
      <div class="mb-3">
        <label>Etisalat Cost</label>
        <input id="responseEtisalatCostInput" type="text" class="form-control" readonly />
      </div>
      <div class="mb-3">
        <label>WE Cost</label>
        <input id="responseWeCostInput" type="text" class="form-control" readonly />
      </div>
      <div class="mb-3">
      </div>
    </div>
  </div>
</div>
